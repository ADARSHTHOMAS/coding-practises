    "def rev_array(list):\n",
    "    l=0\n",
    "    h=len(list)-1\n",
    "    while l<h:\n",
    "        temp=list[l]\n",
    "        list[l]=list[h]\n",
    "        list[h]=temp\n",
    "        l=l+1\n",
    "        h=h-1\n",
    "    else:\n",
    "        print(\"the reverse of array is\",list)\n",
    "list=[1,3,5,7,9,11,13,15]\n",
    "rev_array(list)"
   ]
