    "import math\n",
    "def armstrong_num(n):\n",
    "    sum=0\n",
    "    a=n\n",
    "    while a!=0:\n",
    "        rem=a%10\n",
    "        a=a//10\n",
    "        sum=math.pow(rem,3)+sum\n",
    "    if sum==n:\n",
    "        print(n,\"is a armstrong number\")\n",
    "    else:\n",
    "        print(\"this is not an armstrong number\")\n",
    "armstrong_num(154)"
