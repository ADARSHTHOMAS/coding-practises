    "def star_pyramid(n):\n",
    "    for i in range(1,n+1):\n",
    "        for j in range(1,n+i):\n",
    "            if j<=n-i:\n",
    "                print(\" \",end=\" \")\n",
    "            else:\n",
    "                print(\"* \",end=\"\")\n",
    "        print()\n",
    "star_pyramid(10)\n"
   ]
