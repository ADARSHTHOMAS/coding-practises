    "def hcf_naive(n1,n2):\n",
    "    a=min(n1,n2)\n",
    "    i=1\n",
    "    prod=1\n",
    "    while i<=a:\n",
    "        if n1%i==0 and n2%i==0:\n",
    "            prod=i\n",
    "    print(\"hcf of \",n1,\"& \",n2,\" is \",prod)\n",
    "hcf_naive(2,4)"
   ]
