    "def rec_fact(n):\n",
    "    if n==0:\n",
    "        return 1\n",
    "    prod=rec_fact(n-1)\n",
    "    return prod*n\n",
    "n=10\n",
    "#prod=1\n",
    "factor=rec_fact(n)\n",
    "print(\"factorial of\",n,\"is\",factor)"
   ]
